import os, sys
import gspread 
import requests
import json
from oauth2client.client import SignedJwtAssertionCredentials

json_key = json.load(open('XXXX.json')) #file which holds client email and private key
scope = ['https://spreadsheets.google.com/feeds']

credentials = SignedJwtAssertionCredentials(json_key['client_email'], json_key['private_key'].encode(), scope)
gc = gspread.authorize(credentials)
print('got this far')
try:
   wks = gc.open("Sophi test worksheet").worksheet('XX') 
   #add column with numbers from 1-100
   for num in range(0, 100):
     out_str = out_str+1
     n = n+1
     column = 'H'+ str(out_str)
     column = str(column)
     wks.update_acell (column, n)
     print (column)
   print('yes!')
except:
    print('fail')
sys.exit()
